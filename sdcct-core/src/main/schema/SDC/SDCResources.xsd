                        <xs:annotation>
                            <xs:documentation>**possibly "HH:MM:SS:ms am/pm 12/24"</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="unsignedByte" nillable="true"
                        type="unsignedByte_type"> </xs:element>
                    <xs:element minOccurs="0" name="unsignedInt" nillable="true"
                        type="unsignedInt_type"> </xs:element>
                    <xs:element minOccurs="0" name="unsignedLong" nillable="true"
                        type="unsignedLong_type"> </xs:element>
                    <xs:element minOccurs="0" name="unsignedShort" nillable="true"
                        type="unsignedShort_type"> </xs:element>
                    <xs:element minOccurs="0" name="XML" nillable="true" type="XML_type"> </xs:element>
                    <xs:element name="yearMonthDuration" nillable="true"
                        type="yearMonthDuration_type" minOccurs="0"> </xs:element>
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
