<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
    xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:c="http://www.springframework.org/schema/c"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <!--====================================================================================================
    = FORM MANAGERS
    =====================================================================================================-->
    <beans:bean id="formManager" class="gov.hhs.onc.sdcct.form.manager.impl.FormManagerImpl">
        <beans:property name="internalFormDesignSources">
            <beans:array>
                <!--suppress SpringPlaceholdersInspection -->
                <beans:value>classpath*:${sdcct.data.form.src.dir.path}/adrenal/SDC_Adrenal*.xml</beans:value>
                <!--suppress SpringPlaceholdersInspection -->
                <beans:value>classpath*:${sdcct.data.form.src.dir.path}/test1/form-test1-rfd.xml</beans:value>
            </beans:array>
        </beans:property>
        <beans:property name="internalQuestionnaireSources">
            <beans:array>
                <!--suppress SpringPlaceholdersInspection -->
                <beans:value>classpath*:${sdcct.data.form.src.dir.path}/test1/form-test1-fhir.xml</beans:value>
            </beans:array>
        </beans:property>
    </beans:bean>
</beans:beans>