<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
    xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:c="http://www.springframework.org/schema/c"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--====================================================================================================
    = JAX-WS SERVERS
    =====================================================================================================-->
    <!--suppress SpringPlaceholdersInspection -->
    <beans:bean id="serverFormReceiverRfd" parent="serverRfd"
                p:address="${sdcct.ws.form.receiver.rfd.url.path}"
                p:endpointName="#{ T(gov.hhs.onc.sdcct.rfd.RfdWsXmlQnames).FORM_RECEIVER_PORT }"
                p:serviceBean-ref="wsFormReceiverRfdImpl"
                p:serviceName="#{ T(gov.hhs.onc.sdcct.rfd.RfdWsXmlQnames).FORM_RECEIVER_SERVICE }"
                p:wsdlLocation="${sdcct.ws.form.receiver.rfd.wsdl.loc}"/>

    <!--====================================================================================================
    = JAX-WS CLIENTS
    =====================================================================================================-->
    <!--suppress SpringPlaceholdersInspection -->
    <beans:bean id="clientFormReceiverRfd" parent="clientRfd" lazy-init="true" scope="prototype"
                p:endpointName="#{ T(gov.hhs.onc.sdcct.rfd.RfdWsXmlQnames).FORM_RECEIVER_PORT }"
                p:serviceClass="gov.hhs.onc.sdcct.rfd.RfdFormReceiverPortType"
                p:serviceName="#{ T(gov.hhs.onc.sdcct.rfd.RfdWsXmlQnames).FORM_RECEIVER_SERVICE }"
                p:wsdlLocation="${sdcct.ws.form.receiver.rfd.wsdl.loc}"/>
</beans:beans>